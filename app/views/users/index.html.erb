<p id="notice"><%= notice %></p>

<h1>User Profile</h1>

<% @users.each do |user| %>
    <p>Avatar: <%= user.avatar %></p>
    <p>Name: <%= user.name %></p>
    <p>Email: <%= user.email %></p>
    <p>Password digest: <%= user.password_digest %></p>
    <p>Location: <%= user.location %></p>
    <p>Linkedin url: <%= user.linkedin_url %></p>
    <p>Github url: <%= user.github_url %></p>
    <p>Other site: <%= user.other_site %></p>
    <p>Twitter: <%= user.twitter %></p>
    <p>Slack name: <%= user.slack_name %></p>
    <p><%= link_to 'Show', user %></p>
    <p><%= link_to 'Edit', edit_user_path(user) %></p>
    <p><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></p>
<% end %>


